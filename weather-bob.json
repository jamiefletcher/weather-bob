{
    "version": 8,
    "name": "weather-bob",
    "projection": {
        "type": "globe"
    },
    "sources": {
		"osm": {
			"type": "raster",
			"tiles": ["https://a.tile.openstreetmap.org/{z}/{x}/{y}.png"],
			"tileSize": 256
		}
	},
    "layers": [
        {
            "id": "osm",
            "type": "raster",
            "source": "osm",
            "paint": {
                "raster-hue-rotate": 0,
                "raster-contrast": 0,
                "raster-saturation": 0,
                "raster-brightness-min": 0,
                "raster-brightness-max": 1
            }
        }
    ]
}
